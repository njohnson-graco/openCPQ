    "react": "^17.0.2",
    "react-bootstrap": "^1.5.2",
    "react-widgets": "^5.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "browser-filesaver": "^1.1.1",
    "ncp": "^2.0.0",
    "react": "^17.0.2",
    "react-bootstrap": "^1.5.2",
    "react-widgets": "^5.0.3",
    "rimraf": "^3.0.2"
  }
